<p id="notice"><%= notice %></p>

<h1>Prefectures</h1>

<table>
  <thead>
    <tr>
      <th>Regionid</th>
      <th>Objectid</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prefectures.each do |prefecture| %>
      <tr>
        <td><%= prefecture.regionId %></td>
        <td><%= prefecture.objectId %></td>
        <td><%= prefecture.name %></td>
        <td><%= link_to '保存データを見る', prefecture %></td>
        <td><%= link_to '編集', edit_prefecture_path(prefecture) %></td>
        <td><%# link_to 'Destroy', prefecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '都道府県を追加', new_prefecture_path %>
